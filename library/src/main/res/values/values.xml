<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="HollowTextView"  >
        <attr name="hollow_stroke_startColor" format="color" />
        <attr name="hollow_stroke_centerColor" format="color" />
        <attr name="hollow_stroke_endColor" format="color" />
        <attr name="hollow_stroke_angle" format="float" />
        <attr name="hollow_stroke_rtl_angle" format="boolean" />

        <attr name="hollow_stroke_strokeWidth" format="dimension" />
        <attr name="hollow_stroke_textColor" format="color" />
        <attr name="hollow_stroke_join" format="enum">
            <enum name="miter" value="0" />
            <enum name="round" value="1" />
            <enum name="bevel" value="2" />
        </attr>
    </declare-styleable>
</resources>